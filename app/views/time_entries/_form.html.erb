<%= form_for [@project, @time_entry] do |f| %>
  <%= f.label :hours %>
  <%= f.text_field :hours %>

  <%= f.label :minutes %>
  <%= f.text_field :minutes %>

  <%= f.label :date %>
  <%= f.date_field :date %>

  <%= f.label :comments %>
  <%= f.text_field :comments %>

  <%= f.submit 'Save' %> 
  <%= link_to 'Cancel', project_time_entries_path(@project) %> 
<% end %>